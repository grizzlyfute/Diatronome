    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toBottomOf="@id/playBtn5"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintHeight_percent="1"
    app:layout_constraintStart_toEndOf="@id/playBtn5"
    app:layout_constraintTop_toTopOf="@+id/playBtn5">

    <tag
      android:id="@id/playNoteTag"
      android:value="7" />
  </Button>

  <Button
    android:id="@+id/playBtn9"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_whitetouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="A"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_light"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toBottomOf="@id/playBtn7"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintHeight_percent="1"
