<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="wrap_content">

  <!--
    ||1|3|||6|8|a||
    |0|2|4|5|7|9|b|
    |C|D|E|F|G|A|B|
  -->

  <Button
    android:id="@+id/playBtn0"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_whitetouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="C"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_light"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintHeight_percent="1"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <tag
      android:id="@+id/playNoteTag"
      android:value="0" />
  </Button>

  <Button
    android:id="@+id/playBtn2"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_whitetouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="D"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_light"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toBottomOf="@id/playBtn0"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintHeight_percent="1"
    app:layout_constraintStart_toEndOf="@id/playBtn0"
    app:layout_constraintTop_toTopOf="@+id/playBtn0">

    <tag
      android:id="@id/playNoteTag"
      android:value="2" />
  </Button>

  <Button
    android:id="@+id/playBtn4"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_whitetouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="E"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_light"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toBottomOf="@id/playBtn2"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintHeight_percent="1"
    app:layout_constraintStart_toEndOf="@id/playBtn2"
    app:layout_constraintTop_toTopOf="@+id/playBtn2">

