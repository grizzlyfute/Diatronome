    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="G#/Ab"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_dark"
    android:textSize="12sp"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toTopOf="@id/playHzGuide"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintEnd_toEndOf="@+id/playBtn9"
    app:layout_constraintStart_toStartOf="@+id/playBtn7"
    app:layout_constraintTop_toTopOf="@+id/playBtn7">

    <tag
      android:id="@id/playNoteTag"
      android:value="8" />
  </Button>

  <Button
    android:id="@+id/playBtn10"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_blacktouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="A#/Bb"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_dark"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toTopOf="@id/playHzGuide"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintEnd_toEndOf="@+id/playBtn11"
    app:layout_constraintStart_toStartOf="@+id/playBtn9"
    app:layout_constraintTop_toTopOf="@+id/playBtn9">

    <tag
      android:id="@id/playNoteTag"
      android:value="10" />
  </Button>

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/playHzGuide"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintBottom_toBottomOf="@id/playBtn0"
    app:layout_constraintGuide_percent="0.60"
    app:layout_constraintTop_toTopOf="@id/playBtn0" />

</androidx.constraintlayout.widget.ConstraintLayout>