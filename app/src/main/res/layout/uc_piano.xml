    app:layout_constraintTop_toTopOf="@+id/playBtn7">

    <tag
      android:id="@id/playNoteTag"
      android:value="9" />
  </Button>

  <Button
    android:id="@+id/playBtn11"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_whitetouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:tag="note=11"
    android:text="B"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_light"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toBottomOf="@id/playBtn9"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHeight_percent="1"
    app:layout_constraintStart_toEndOf="@id/playBtn9"
    app:layout_constraintTop_toTopOf="@+id/playBtn9">

    <tag
      android:id="@id/playNoteTag"
      android:value="11" />
  </Button>

  <Button
    android:id="@+id/playBtn1"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_blacktouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="C#/Db"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_dark"
    android:textSize="12sp"
    android:stateListAnimator="@null"
    app:backgroundTint="@null"
    app:layout_constraintBottom_toTopOf="@id/playHzGuide"
    app:layout_constraintDimensionRatio="1:2"
    app:layout_constraintEnd_toEndOf="@+id/playBtn2"
    app:layout_constraintStart_toStartOf="@+id/playBtn0"
    app:layout_constraintTop_toTopOf="@+id/playBtn0">

    <tag
      android:id="@id/playNoteTag"
      android:value="1" />
  </Button>

  <Button
    android:id="@+id/playBtn3"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/piano_blacktouch"
    android:gravity="bottom|center_horizontal"
    android:paddingBottom="8dp"
    android:text="D#/Eb"
    android:textAllCaps="false"
    android:textColor="@android:color/primary_text_dark"
